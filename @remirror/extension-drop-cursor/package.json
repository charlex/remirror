{
  "name": "@remirror/extension-drop-cursor",
  "description": "CodeBlock extension for the remirror wysiwyg editor",
  "license": "MIT",
  "author": "Ifiok Jr. <ifiokotung@gmail.com>",
  "homepage": "https://github.com/ifiokjr/remirror/tree/master/@remirror/extension-drop-cursor",
  "repository": "https://github.com/ifiokjr/remirror/tree/master/@remirror/extension-drop-cursor",
  "version": "0.4.2-canary.2",
  "main": "lib/index.js",
  "module": "lib/dist/extension-drop-cursor.esm.js",
  "files": [
    "internal",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:babel": "babel src --out-dir lib --root-mode=upward --extensions \".ts,.tsx\" -s",
    "build:declaration": "tsc -p ./tsconfig.prod.json --emitDeclarationOnly --declarationMap",
    "dev": "run-p dev:*",
    "dev:babel": "babel src --out-dir lib --root-mode=upward --extensions \".ts,.tsx\" -s --watch",
    "dev:declaration": "tsc -p ./tsconfig.prod.json --emitDeclarationOnly --declarationMap --watch",
    "lint": "tslint --project tsconfig.lint.json --config ../../tslint.json",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "@remirror/core": "0.4.2-canary.2",
    "@remirror/react-portals": "0.4.2-canary.2",
    "@remirror/react-utils": "0.4.2-canary.2",
    "@remirror/ui": "0.4.2-canary.2",
    "@types/prosemirror-state": "^1.2.3",
    "@types/prosemirror-transform": "^1.1.0",
    "@types/prosemirror-view": "^1.9.0",
    "prosemirror-state": "^1.2.4",
    "prosemirror-transform": "^1.1.4",
    "prosemirror-view": "^1.11.2"
  },
  "peerDependencies": {
    "@emotion/core": "^10",
    "@types/react": "^16.8.0",
    "react": "^16.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "cjs": "lib/dist/extension-drop-cursor.cjs.js",
  "meta": {
    "sizeLimit": "90 KB"
  },
  "sideEffects": false
}
