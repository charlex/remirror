{
  "name": "@remirror/multishift",
  "description": "The dropdown component used throughout the codebase",
  "license": "MIT",
  "author": "Ifiok Jr. <ifiokotung@gmail.com>",
  "homepage": "https://github.com/ifiokjr/remirror/tree/master/@remirror/multishift",
  "repository": "https://github.com/ifiokjr/remirror/tree/master/@remirror/multishift",
  "version": "0.4.2-canary.2",
  "main": "lib/index.js",
  "module": "lib/dist/multishift.esm.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:babel": "babel src --out-dir lib --root-mode=upward --extensions \".ts,.tsx\" -s",
    "build:declaration": "tsc -p ./tsconfig.prod.json --emitDeclarationOnly --declarationMap",
    "dev": "run-p dev:*",
    "dev:babel": "babel src --out-dir lib --root-mode=upward --extensions \".ts,.tsx\" -s --watch",
    "dev:declaration": "tsc -p ./tsconfig.prod.json --emitDeclarationOnly --declarationMap --watch",
    "lint": "tslint --project tsconfig.lint.json --config ../../tslint.json",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@reach/auto-id": "0.2.0",
    "@reach/visually-hidden": "^0.1.4",
    "@remirror/core-helpers": "0.4.2-canary.2",
    "@remirror/core-types": "0.4.2-canary.2",
    "@remirror/react-hooks": "0.4.2-canary.2",
    "@remirror/react-utils": "0.4.2-canary.2",
    "@remirror/ui": "0.4.2-canary.2",
    "@remirror/ui-buttons": "0.4.2-canary.2",
    "@remirror/ui-icons": "0.4.2-canary.2",
    "@remirror/ui-text": "0.4.2-canary.2",
    "@types/reach__auto-id": "0.2.0",
    "@types/reach__visually-hidden": "^0.2.0",
    "@seznam/compose-react-refs": "1.0.2",
    "compute-scroll-into-view": "1.0.11",
    "downshift": "^3.2.12",
    "react-spring": "8.0.27"
  },
  "peerDependencies": {
    "@emotion/core": "^10",
    "@types/react": "^16.8.0",
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@remirror/core-extensions": "0.4.2-canary.2",
    "prosemirror-model": "^1.7.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "cjs": "lib/dist/multishift.cjs.js",
  "meta": {
    "sizeLimit": "70 KB"
  }
}
