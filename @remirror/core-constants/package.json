{
  "name": "@remirror/core-constants",
  "description": "The core constants used throughout the remirror codebase",
  "license": "MIT",
  "author": "Ifiok Jr. <ifiokotung@gmail.com>",
  "homepage": "https://github.com/ifiokjr/remirror/tree/master/@remirror/core-constants",
  "repository": "https://github.com/ifiokjr/remirror/tree/master/@remirror/core-constants",
  "version": "0.4.2-canary.2",
  "main": "lib/index.js",
  "module": "lib/dist/core-constants.esm.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:babel": "babel src --out-dir lib --root-mode=upward --extensions \".ts,.tsx\" -s",
    "build:declaration": "tsc -p ./tsconfig.prod.json --emitDeclarationOnly --declarationMap",
    "dev": "run-p dev:*",
    "dev:babel": "babel src --out-dir lib --root-mode=upward --extensions \".ts,.tsx\" -s --watch",
    "dev:declaration": "tsc -p ./tsconfig.prod.json --emitDeclarationOnly --declarationMap --watch",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@babel/runtime": "^7.6.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "cjs": "lib/dist/core-constants.cjs.js",
  "meta": {
    "sizeLimit": "2 KB"
  },
  "sideEffects": false
}
