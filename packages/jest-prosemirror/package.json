    "@types/prosemirror-schema-list": "^1.0.1",
    "@types/prosemirror-tables": "^0.8.0",
    "@types/prosemirror-test-builder": "^1.0.1",
    "@types/prosemirror-view": "^1.9.0",
    "chalk": "^2.4.2",
    "jest-diff": "^24.9.0",
